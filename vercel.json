{
  "version": 2,
  "builds": [
    { 
      "src": "main.py", 
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "main.py" }
  ],
  "functions": {
    "main.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "env": {
    "PYTHONPATH": "."
  }
} 